import java.util.Scanner;

public class N2675 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int T = sc.nextInt(); // 테스트 케이스 개수
		int[] arr = null;
		
		for(int i = 0; i < T; i++) {
			
			int R = sc.nextInt(); // 반복할 길이
			String S = sc.nextLine(); // 입력받는 단어
			arr[i] = S.charAt(i); // 단어 조각내기
						
			for(int j = 0; j < R; j++) {
				
				
			}
		}

	}

}
