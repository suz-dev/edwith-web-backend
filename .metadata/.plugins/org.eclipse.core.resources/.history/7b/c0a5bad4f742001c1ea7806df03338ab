import java.util.Scanner;

public class N1152 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);		
		String S = sc.nextLine();
		
		int cnt = 0;
		
		for(int i = 0; i < S.length(); i++) {
			if(S.charAt(i) == 32 ) {
				cnt++;			
			}
			
		}
		System.out.println(cnt);
	}

}
