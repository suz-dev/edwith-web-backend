import java.util.Scanner;

public class N10809 {
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String s = sc.next();
		
		int[] arr = new int[26];
		
		for(int i = 0; i < s.length(); i++) {
			
			for(int j = 0; j < arr.length; j++) {
				
				switch(s.charAt(i)) {
				case 'a': arr[0] = i;
				case 'b': arr[1] = i;
				case 'c': arr[2] = i;
				case 'd': arr[3] = i;
				case 'e': arr[4] = i;
				case 'f': arr[5] = i;
				case 'g': arr[6] = i;
				case 'h': arr[7] = i;
				case 'i': arr[8] = i;
				case 'j': arr[9] = i;
				case 'k': arr[10] = i;
				case 'l': arr[11] = i;
				case 'm': arr[12] = i;
				case 'n': arr[13] = i;
				case 'o': arr[14] = i;
				case 'p': arr[15] = i;
				case 'q': arr[16] = i;
				case 'r': arr[17] = i;
				case 's': arr[18] = i;
				case 't': arr[19] = i;
				case 'u': arr[20] = i;
				case 'v': arr[21] = i;
				case 'w': arr[22] = i;
				case 'x': arr[23] = i;
				case 'y': arr[24] = i;
				case 'z': arr[25] = i;
//				default: arr[j] = -1;
				break;
				}
				
			}
			System.out.println(arr[i]);
		}
	}
}   
